{
  "path": "android.view.inputmethod",
  "name": "EditorInfo",
  "values": [
    {
      "type": "constant"
    },
    {
      "type": "constant",
      "keyword": "static",
      "identifier": "Int",
      "code": "IME_ACTION_DONE",
      "desc": "Bits of IME_MASK_ACTION: the action key performs a \"done\" operation, typically meaning there is nothing more to input and the IME will be closed."
    },
    {
      "type": "constant",
      "keyword": "static",
      "identifier": "Int",
      "code": "IME_ACTION_GO",
      "desc": "Bits of IME_MASK_ACTION: the action key performs a \"go\" operation to take the user to the target of the text they typed."
    },
    {
      "type": "constant",
      "keyword": "static",
      "identifier": "Int",
      "code": "IME_ACTION_NEXT",
      "desc": "Bits of IME_MASK_ACTION: the action key performs a \"next\" operation, taking the user to the next field that will accept text."
    },
    {
      "type": "constant",
      "keyword": "static",
      "identifier": "Int",
      "code": "IME_ACTION_NONE",
      "desc": "Bits of IME_MASK_ACTION: there is no available action."
    },
    {
      "type": "constant",
      "keyword": "static",
      "identifier": "Int",
      "code": "IME_ACTION_PREVIOUS",
      "desc": "Bits of IME_MASK_ACTION: like IME_ACTION_NEXT, but for moving to the previous field."
    },
    {
      "type": "constant",
      "keyword": "static",
      "identifier": "Int",
      "code": "IME_ACTION_SEARCH",
      "desc": "Bits of IME_MASK_ACTION: the action key performs a \"search\" operation, taking the user to the results of searching for the text they have typed (in whatever context is appropriate)."
    },
    {
      "type": "constant",
      "keyword": "static",
      "identifier": "Int",
      "code": "IME_ACTION_SEND",
      "desc": "Bits of IME_MASK_ACTION: the action key performs a \"send\" operation, delivering the text to its target."
    },
    {
      "type": "constant",
      "keyword": "static",
      "identifier": "Int",
      "code": "IME_ACTION_UNSPECIFIED",
      "desc": "Bits of IME_MASK_ACTION: no specific action has been associated with this editor, let the editor come up with its own if it can."
    },
    {
      "type": "constant",
      "keyword": "static",
      "identifier": "Int",
      "code": "IME_FLAG_FORCE_ASCII",
      "desc": "Flag of imeOptions: used to request an IME that is capable of inputting ASCII characters."
    },
    {
      "type": "constant",
      "keyword": "static",
      "identifier": "Int",
      "code": "IME_FLAG_NAVIGATE_NEXT",
      "desc": "Flag of imeOptions: used to specify that there is something interesting that a forward navigation can focus on."
    },
    {
      "type": "constant",
      "keyword": "static",
      "identifier": "Int",
      "code": "IME_FLAG_NAVIGATE_PREVIOUS",
      "desc": "Flag of imeOptions: like IME_FLAG_NAVIGATE_NEXT, but specifies there is something interesting that a backward navigation can focus on."
    },
    {
      "type": "constant",
      "keyword": "static",
      "identifier": "Int",
      "code": "IME_FLAG_NO_ACCESSORY_ACTION",
      "desc": "Flag of imeOptions: used in conjunction with one of the actions masked by IME_MASK_ACTION, this indicates that the action should not be available as an accessory button on the right of the extracted text when the input method is full-screen."
    },
    {
      "type": "constant",
      "keyword": "static",
      "identifier": "Int",
      "code": "IME_FLAG_NO_ENTER_ACTION",
      "desc": "Flag of imeOptions: used in conjunction with one of the actions masked by IME_MASK_ACTION."
    },
    {
      "type": "constant",
      "keyword": "static",
      "identifier": "Int",
      "code": "IME_FLAG_NO_EXTRACT_UI",
      "desc": "Flag of imeOptions: used to specify that the IME does not need to show its extracted text UI."
    },
    {
      "type": "constant",
      "keyword": "static",
      "identifier": "Int",
      "code": "IME_FLAG_NO_FULLSCREEN",
      "desc": "Flag of imeOptions: used to request that the IME never go into fullscreen mode."
    },
    {
      "type": "constant",
      "keyword": "static",
      "identifier": "Int",
      "code": "IME_FLAG_NO_PERSONALIZED_LEARNING",
      "desc": "Flag of imeOptions: used to request that the IME should not update any personalized data such as typing history and personalized language model based on what the user typed on this text editing object."
    },
    {
      "type": "constant",
      "keyword": "static",
      "identifier": "Int",
      "code": "IME_MASK_ACTION",
      "desc": "Set of bits in imeOptions that provide alternative actions associated with the \"enter\" key."
    },
    {
      "type": "constant",
      "keyword": "static",
      "identifier": "Int",
      "code": "IME_NULL",
      "desc": "Generic unspecified type for imeOptions."
    },

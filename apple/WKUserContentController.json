{
  "type": "Class",
  "name": "WKUserContentController",
  "desc": "A WKUserContentController object provides a way for JavaScript to post messages and inject user scripts to a web view.",
  "items": [
    {
      "name": "add",
      "type": "func",
      "desc": "Adds a script message handler.",
      "params": [
        {
          "type": "WKScriptMessageHandler"
        },
        {
          "name": "name",
          "type": "String"
        }
      ]
    },
    {
      "name": "addUserScript",
      "type": "func",
      "desc": "Adds a user script.",
      "params": [
        {
          "type": "WKUserScript"
        }
      ]
    },
    {
      "name": "removeAllUserScripts",
      "type": "func",
      "desc": "Removes all associated user scripts.",
      "params": []
    },
    {
      "name": "removeScriptMessageHandler",
      "type": "func",
      "desc": "Removes a script message handler.",
      "params": [
        {
          "name": "forName",
          "type": "String"
        }
      ]
    },
    {
      "name": "userScripts",
      "type": "var",
      "datatype": "[WKUserScript]",
      "desc": "The user scripts associated with the user content controller."
    },
    {
      "name": "add",
      "type": "func",
      "desc": "Adds a list of content rules.",
      "params": [
        {
          "type": "WKContentRuleList"
        }
      ]
    },
    {
      "name": "remove",
      "type": "func",
      "desc": "Removes a rules list.",
      "params": [
        {
          "type": "WKContentRuleList"
        }
      ]
    },
    {
      "name": "removeAllContentRuleLists",
      "type": "func",
      "desc": "Removes all rules lists.",
      "params": []
    }
  ]
}
